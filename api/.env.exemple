# 🌐 Port du serveur
PORT=3000

# 🛢️ URL de connexion PostgreSQL
# Format : postgresql://USER:PASSWORD@HOST:PORT/DATABASE
DATABASE_URL=mysql://your_user:your_password@localhost:3306/your_database

# 🔐 JWT (Token d'authentification)
JWT_SECRET=your_super_secret_key
JWT_EXPIRES_IN=1d
# Note: Replace 'your_user', 'your_password', 'localhost', '5432', and 'your_database' with your actual PostgreSQL credentials.